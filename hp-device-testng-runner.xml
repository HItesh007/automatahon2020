<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Mobile Automation - Research Suite" parallel="tests">
    <parameter name="appium-server-ip" value="127.0.0.1"/>

    <test name="Research Test - Moto G5s Plus">
        <parameter name="device-name" value="Moto G5s Plus"/>
        <parameter name="platform-name" value="Android"/>
        <parameter name="platform-version" value="7.1.1"/>
        <parameter name="udid" value="ZY322JJLX7"/>
        <parameter name="automation-name" value="UiAutomator2"/>
        <parameter name="wda-system-port" value="8201"/>
        <parameter name="browser-name" value="Chrome"/>
        <parameter name="app-package" value="net.metaquotes.metatrader4"/>
        <parameter name="app-activity" value="net.metaquotes.metatrader4.ui.MainActivity"/>
        <classes>
            <class name="com.hitesh.automatahon.builder.appium.session.AppiumSessionBuilder"/>
            <!--<class name="BoilerplateChromeTests"/>-->
        </classes>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Boilerplate Suite - Native App Suite" parallel="tests">
    <parameter name="appium-server-ip" value="127.0.0.1"/>

    <test name="Calculator Test - Moto G5s Plus">
        <parameter name="device-name" value="Moto G5s Plus"/>
        <parameter name="platform-name" value="Android"/>
        <parameter name="platform-version" value="10.0"/>
        <parameter name="udid" value="ZY322JJLX7"/>
        <parameter name="automation-name" value="UiAutomator2"/>
        <parameter name="wda-system-port" value="8201"/>
        <parameter name="app-package" value="com.google.android.calculator"/>
        <parameter name="app-activity" value="com.android.calculator2.Calculator"/>
        <classes>
            <class name="com.hitesh.automatahon.builder.appium.session.AppiumSessionBuilder"/>
            <class name="com.hitesh.automatahon.tests.boilerplate.calc.tests.BoilerplateCalculatorTests"/>
        </classes>
    </test>
</suite>